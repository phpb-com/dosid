  "dependencies": {
    "hashids": "2.2.11"
  },
  "lint-staged": {
    "*.{ts,md,json}": "prettier --write",
    "*.ts": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "packageManager": "yarn@3.5.0"
